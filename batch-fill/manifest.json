{
    "name": "Batch Fill",
    "description": "A tool helping filling a lot of fields",
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["cs.js"],
            "all_frames": true
        }
    ],
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ],
    "icons": {
        "128": "icon-128.png",
        "256": "icon.png"
    },
    "options_page": "options.html",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "version": "1.2",
    "manifest_version": 2
}
